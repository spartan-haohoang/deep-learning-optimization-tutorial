# Deep Learning Optimization Tutorial Environment Variables
# Copy this file to .env and update the values as needed

# Jupyter Configuration
JUPYTER_ENABLE_LAB=yes
JUPYTER_TOKEN=
JUPYTER_PASSWORD=

# Development Settings
DEBUG=false
LOG_LEVEL=INFO

# Data Paths
DATA_DIR=./data
MODELS_DIR=./models
LOGS_DIR=./logs

# Testing Configuration
PYTEST_TIMEOUT=300
COVERAGE_THRESHOLD=80

# Docker Configuration
DOCKER_IMAGE_NAME=deep-learning-optimization
DOCKER_TAG=latest

# GitHub Configuration (for CI/CD)
GITHUB_TOKEN=
CODECOV_TOKEN=
